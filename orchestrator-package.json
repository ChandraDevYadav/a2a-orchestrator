{
  "name": "quiz-orchestrator",
  "version": "1.0.0",
  "description": "Standalone A2A Orchestrator Service",
  "main": "orchestrator-server.js",
  "scripts": {
    "start": "node orchestrator-server.js",
    "dev": "nodemon orchestrator-server.js",
    "build": "npm run build:frontend && npm run build:orchestrator",
