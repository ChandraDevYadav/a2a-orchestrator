    "cors": "^2.8.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.2"
  },
  "keywords": ["a2a", "orchestrator", "agent", "quiz"],
  "author": "Your Name",
  "license": "MIT"
}
